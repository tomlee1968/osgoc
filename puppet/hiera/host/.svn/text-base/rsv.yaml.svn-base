---
hiera::certificate:
  rsv.grid.iu.edu:
    cert:
      path: /etc/grid-security/http/cilogon-cert.pem
      source: puppet:///modules/hiera/certificates/rsv.grid.iu.edu_http_cert.pem
      owner: daemon
      group: daemon
      mode: 0644
      replace: true
      notify:
        - Exec[httpd_condrestart]
    key:
      path: /etc/grid-security/http/cilogon-key.pem
      content: |
        ENC[PKCS7,]
      owner: daemon
      group: daemon
      mode: 0600
      replace: true
      notify:
        - Exec[httpd_condrestart]

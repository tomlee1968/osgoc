# Resources specifically for host oim-dev2.grid.iu.edu
---
hiera::certificate:
  oim-dev.grid.iu.edu:
    cert:
      path: /etc/grid-security/host/cert.pem
      source: puppet:///modules/hiera/certificates/oim-dev.grid.iu.edu_cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      path: /etc/grid-security/host/key.pem
      content: |
        ENC[PKCS7,]
      owner: root
      group: root
      mode: 0600
      replace: true
    require:
      - File[/etc/grid-security/host]
  oim-dev.grid.iu.edu_2:
    cert:
      path: /etc/grid-security/http/cert.pem
      source: puppet:///modules/hiera/certificates/oim-dev.grid.iu.edu_cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      path: /etc/grid-security/http/key.pem
      content: |
        ENC[PKCS7,]
      owner: root
      group: root
      mode: 0600
      replace: true
    require:
      - File[/etc/grid-security/http]

hiera::file:
  /etc/grid-security/http:
    ensure: directory
    owner: root
    group: root
    mode: 0755

---
hiera::certificate:
  oasis-login.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/oasis-login.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true

hiera::service:
  gsisshd:
    ensure: running
    enable: true
    hasrestart: true
    hasstatus: true

hiera::service_exclude:
  - sshd

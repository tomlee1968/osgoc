---
hiera::certificate:
  imap.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/imap.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
      notify:
        - Service[dovecot]
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true
      notify:
        - Service[dovecot]

#    enable: false
#    hasrestart: true
#    hasstatus: true
#  postgresql-9.6:
#    ensure: running
#    enable: true
#    hasrestart: true
#    hasstatus: true
  simplevisor:
    ensure: running
    enable: true
    hasrestart: true
    hasstatus: true

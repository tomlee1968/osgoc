---
hiera::certificate:
  is-itb1.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/is-itb1.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
      notify:
        - Exec[httpd_condrestart]
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true
hiera::exec:
  httpd_condrestart:
    command: /sbin/service httpd condrestart
    refreshonly: true

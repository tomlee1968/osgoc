---
hiera::certificate:
  psds0.grid.iu.edu:
    cert:
      path: /etc/grid-security/host/cert.pem
      source: puppet:///modules/hiera/certificates/psds0.grid.iu.edu_cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      path: /etc/grid-security/host/key.pem
      content: |
        ENC[PKCS7,]
      owner: root
      group: root
      mode: 0600
      replace: true

hiera::service:
  postgresql-9.4:
    ensure: stopped
    enable: false
    hasrestart: true
    hasstatus: true
  postgresql-9.6:
    ensure: running
    enable: true
    hasrestart: true
    hasstatus: true
  simplevisor:
    ensure: running
    enable: true
    hasrestart: true
    hasstatus: true

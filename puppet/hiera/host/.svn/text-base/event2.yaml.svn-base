# Resources specifically for host event2.grid.iu.edu
---
hiera::certificate:
  event2.grid.iu.edu:
    cert:
      path: /etc/grid-security/host/cert.pem
      source: puppet:///modules/hiera/certificates/event.grid.iu.edu_cert.pem
      owner: rabbitmq
      group: rabbitmq
      mode: 0644
      replace: true
    key:
      path: /etc/grid-security/host/key.pem
      content: |
        ENC[PKCS7,]
      owner: rabbitmq
      group: rabbitmq
      mode: 0600
      replace: true

hiera::file:
  /etc/grid-security/intermediate:
    ensure: directory
    owner: root
    group: root
    mode: 0755
  /etc/grid-security/intermediate/incommon.pem:
    source: puppet:///modules/hiera/certificates/intermediate_incommon.pem
    ensure: present
    owner: root
    group: root
    mode: 0644
    replace: true

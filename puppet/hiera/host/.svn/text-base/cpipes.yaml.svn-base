---
hiera::certificate:
  cpipes.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/cpipes.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true
  'http/cpipes.grid.iu.edu':
    cert:
      source: puppet:///modules/hiera/certificates/cpipes.grid.iu.edu_http_cert.pem
      path: /etc/grid-security/http/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
      notify:
        - Exec[httpd_condrestart]
    key:
      path: /etc/grid-security/http/key.pem
      content: |
        ENC[PKCS7,]
      owner: root
      group: root
      mode: 0600
      replace: true
  'rsv/cpipes.grid.iu.edu':
    cert:
      source: puppet:///modules/hiera/certificates/cpipes.grid.iu.edu_rsv_cert.pem
      path: /etc/grid-security/rsv/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/rsv/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true

hiera::exec:
  httpd_condrestart:
    command: /sbin/service httpd condrestart
    refreshonly: true

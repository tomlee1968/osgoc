# Resources specifically for host meshconfig.grid.iu.edu
---
hiera::certificate:
  meshconfig.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/meshconfig.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true

ssh_userkeys::keys:
  "Shawn's keys":
    type: ssh-dss
    key: 

ssh_userkeys::users:
  smckee:
    home: /home/smckee
    group: smckee
    keys:
      - "Shawn's keys"

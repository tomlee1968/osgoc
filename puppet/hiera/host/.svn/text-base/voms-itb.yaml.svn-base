---
hiera::certificate:
  voms-itb.grid.iu.edu:
    cert:
      source: puppet:///modules/hiera/certificates/voms-itb.grid.iu.edu_cert.pem
      path: /etc/grid-security/host/cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/host/key.pem
      owner: root
      group: root
      mode: 0600
      replace: true
  voms-itb.grid.iu.edu_2:
    cert:
      source: puppet:///modules/hiera/certificates/voms-itb.grid.iu.edu_cert.pem
      path: /etc/grid-security/hostcert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/hostkey.pem
      owner: root
      group: root
      mode: 0600
      replace: true
  voms-itb.grid.iu.edu_3:
    cert:
      source: puppet:///modules/hiera/certificates/voms-itb.grid.iu.edu_cert.pem
      path: /etc/grid-security/voms/vomscert.pem
      owner: voms
      group: voms
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/voms/vomskey.pem
      owner: voms
      group: voms
      mode: 0600
      replace: true
  'http/voms-itb.grid.iu.edu':
    cert:
      source: puppet:///modules/hiera/certificates/voms-itb.grid.iu.edu_http_cert.pem
      path: /etc/grid-security/http/cert.pem
      owner: tomcat
      group: tomcat
      mode: 0644
      replace: true
      notify:
        - Exec[tomcat6_condrestart]
    key:
      path: /etc/grid-security/http/key.pem
      content: |
        ENC[PKCS7,]
      owner: tomcat
      group: tomcat
      mode: 0600
      replace: true
    require:
      - File[/etc/grid-security/http]
  'http/voms-itb.grid.iu.edu_2':
    cert:
      source: puppet:///modules/hiera/certificates/voms-itb.grid.iu.edu_http_cert.pem
      path: /etc/grid-security/http-voms-itb.grid.iu.edu-cert.pem
      owner: root
      group: root
      mode: 0644
      replace: true
    key:
      content: |
        ENC[PKCS7,]
      path: /etc/grid-security/http-voms-itb.grid.iu.edu-key.pem
      owner: root
      group: root
      mode: 0600
      replace: true

hiera::exec:
  tomcat6_condrestart:
    command: /sbin/service tomcat6 condrestart
    refreshonly: true

hiera::file:
  /etc/grid-security/http:
    ensure: directory
    owner: root
    group: root
    mode: 0755
  /etc/grid-security/voms:
    ensure: directory
    owner: root
    group: root
    mode: 0755
